{
  "id": "FreeRTOS_Version",
  "title": "Amazon FreeRTOS Version Test",
  "details": "this test case will check the FreeRTOS version",
  "execution": {
    "timeout": 30000,
    "mac": {
      "cmd": "./../../../afq_mac_x86-64",
      "args": [
        "run-test",
        "--test-group",
        "AmazonFreeRTOSVersion",
        "--device",
        "{\"board\":\"{{device.id}}\", \"uartConnectivity\" : { \"serialPort\" : \"{{device.connectivity.serialPort}}\"}}",
        "--src-path",
        "{{userData.sourcePath}}",
        "--test-artifacts-path",
        "{{testData.testArtifactsPath}}",
        "--test-tmp-src-code-path",
        "{{testData.sourcePath}}"
      ]
    },
    "linux": {
      "cmd": "./../../../afq_linux_x86-64",
      "args": [
        "run-test",
        "--test-group",
        "AmazonFreeRTOSVersion",
        "--device",
        "{\"board\":\"{{device.id}}\", \"uartConnectivity\" : { \"serialPort\" : \"{{device.connectivity.serialPort}}\"}}",
        "--src-path",
        "{{userData.sourcePath}}",
        "--test-artifacts-path",
        "{{testData.testArtifactsPath}}",
        "--test-tmp-src-code-path",
        "{{testData.sourcePath}}"
      ]
    },
    "win": {
      "cmd": "./../../../afq_win_x86-64.exe",
      "args": [
        "run-test",
        "--test-group",
        "AmazonFreeRTOSVersion",
        "--device",
        "{\"board\":\"{{device.id}}\", \"uartConnectivity\" : { \"serialPort\" : \"{{device.connectivity.serialPort}}\"}}",
        "--src-path",
        "{{userData.sourcePath}}",
        "--test-artifacts-path",
        "{{testData.testArtifactsPath}}",
        "--test-tmp-src-code-path",
        "{{testData.sourcePath}}"
      ]
    }
  }
}
